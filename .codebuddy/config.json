{
  "servers": [
    {
      "extensions": [
        "js",
        "ts",
        "jsx",
        "tsx"
      ],
      "command": [
        "npx",
        "--",
        "typescript-language-server",
        "--stdio"
      ],
      "rootDir": "."
    },
    {
      "extensions": [
        "py",
        "pyi"
      ],
      "command": [
        "pylsp"
      ],
      "rootDir": ".",
      "restartInterval": 5,
      "initializationOptions": {
        "settings": {
          "pylsp": {
            "plugins": {
              "jedi_completion": {
                "enabled": true
              },
              "jedi_definition": {
                "enabled": true
              },
              "jedi_hover": {
                "enabled": true
              },
              "jedi_references": {
                "enabled": true
              },
              "jedi_signature_help": {
                "enabled": true
              },
              "jedi_symbols": {
                "enabled": true
              },
              "pylint": {
                "enabled": false
              },
              "pycodestyle": {
                "enabled": false
              },
              "pyflakes": {
                "enabled": false
              },
              "yapf": {
                "enabled": false
              },
              "rope_completion": {
                "enabled": true
              },
              "rope_rename": {
                "enabled": true
              }
            }
          }
        }
      }
    },
    {
      "extensions": [
        "go"
      ],
      "command": [
        "/home/developer/go/bin/gopls"
      ],
      "rootDir": "."
    },
    {
      "extensions": [
        "rs"
      ],
      "command": [
        "/home/developer/.cargo/bin/rust-analyzer"
      ],
      "rootDir": "."
    },
    {
      "extensions": [
        "json",
        "jsonc"
      ],
      "command": [
        "npx",
        "--",
        "vscode-json-language-server",
        "--stdio"
      ],
      "rootDir": "."
    },
    {
      "extensions": [
        "html",
        "htm"
      ],
      "command": [
        "npx",
        "--",
        "vscode-html-language-server",
        "--stdio"
      ],
      "rootDir": "."
    },
    {
      "extensions": [
        "yaml",
        "yml"
      ],
      "command": [
        "npx",
        "--",
        "yaml-language-server",
        "--stdio"
      ],
      "rootDir": "."
    },
    {
      "extensions": [
        "sh",
        "bash",
        "zsh"
      ],
      "command": [
        "npx",
        "--",
        "bash-language-server",
        "start"
      ],
      "rootDir": "."
    }
  ]
}